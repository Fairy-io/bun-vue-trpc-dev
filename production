apiVersion: apps/v1
kind: Deployment
metadata:
    name: test
spec:
    selector:
        matchLabels:
            app: test
    template:
        metadata:
            labels:
                app: test
        spec:
            containers:
                - name: test
                  image: image:0.0.1
                  ports:
                      - name: http
                        containerPort: 3000
                        protocol: TCP
