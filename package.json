{
  "name": "bun-vue-trpc-dev",
  "version": "0.0.0",
  "devDependencies": {
    "@storybook/addon-essentials": "^7.5.3",
    "@storybook/addon-interactions": "^7.5.3",
    "@storybook/addon-links": "^7.5.3",
    "@storybook/blocks": "^7.5.3",
    "@storybook/testing-library": "^0.2.2",
    "@storybook/vue3": "^7.5.3",
    "@storybook/vue3-vite": "^7.5.3",
    "@types/cors": "^2.8.16",
    "@types/express": "^4.17.21",
    "@vitejs/plugin-vue": "^4.2.3",
    "bun-types": "^1.0.18",
    "concurrently": "^8.2.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook": "^7.5.3",
    "typescript": "^5.0.2",
    "vite": "^4.4.5",
    "vue-tsc": "^1.8.5"
  },
  "private": true,
  "scripts": {
    "dev": "concurrently -n \"BACKEND,FRONTEND,STORYBOOK\" -c \"bgYellow.bold,bgGreen.bold,bgWhite.bold\" \"bun backend:dev\" \"bun frontend:dev\" \"bun storybook\"",
    "backend:dev": "bun --hot run ./src/backend",
    "frontend:dev": "vite",
    "vue:build": "vue-tsc && vite build",
    "server:build": "bun build ./src/backend.ts --outfile=server.js --sourcemap --target=bun --minify",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "type": "module",
  "workspaces": [
    "packages/*"
  ]
}
